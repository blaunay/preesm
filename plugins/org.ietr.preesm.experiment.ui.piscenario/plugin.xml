<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.ietr.preesm.category"
            class="org.ietr.preesm.experiment.ui.piscenario.wizard.NewPiScenarioFileWizard"
            icon="icon/preesm5mini.png"
            id="org.ietr.preesm.experiment.ui.piscenario.wizard.NewPiScenarioFileWizard"
            name="PiScenario">
         <description>
            Create a new PiScenario file
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.ietr.preesm.experiment.ui.piscenario.editor.PiScenarioEditor"
            default="true"
            extensions="piscenario"
            icon="icon/preesm5mini.png"
            id="org.ietr.preesm.experiment.ui.piscenario.editor.PiScenarioEditor"
            name="PiScenario Editor">
      </editor>
   </extension>

</plugin>
