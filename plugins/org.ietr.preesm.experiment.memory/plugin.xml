<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.ietr.dftools.workflow.tasks">
      <task
            id="org.ietr.preesm.experiment.memory.MultiMemoryExclusionGraphBuilder"
            name="Multi Memory Exclusion Graph Builder"
            type="org.ietr.preesm.experiment.memory.MultiMemoryExclusionGraphBuilder">
         <inputs>
            <input
                  id="scenario"
                  object="org.ietr.preesm.core.scenario.PreesmScenario">
            </input>
            <input
                  id="DAGs"
                  object="java.util.Set&lt;org.ietr.dftools.algorithm.model.dag.DirectedAcyclicGraph&gt;">
            </input>
         </inputs>
         <outputs>
            <output
                  id="MemExs"
                  object="java.util.Set&lt;org.ietr.preesm.memory.exclusiongraph.MemoryExclusionGraph&gt;">
            </output>
         </outputs>
      </task>
   </extension>

</plugin>
