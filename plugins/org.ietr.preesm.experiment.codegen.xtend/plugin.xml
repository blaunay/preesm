<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.ietr.dftools.workflow.tasks">
      <task
            id="org.ietr.preesm.experiment.codegen.xtend.MultiCodegenTask"
            name="Multi Xtend Code Generation"
            type="org.ietr.preesm.experiment.codegen.xtend.MultiCodegenTask">
         <inputs>
            <input
                  id="scenario"
                  object="org.ietr.preesm.core.scenario.PreesmScenario">
            </input>
            <input
                  id="architecture"
                  object="org.ietr.dftools.architecture.slam.Design">
            </input>
            <input
                  id="DAGsAndMemExs"
                  object="java.util.Map&lt;DirectedAcyclicGraph, MemoryExclusionGraph&gt;">
            </input>
         </inputs>
      </task>
   </extension>

</plugin>
