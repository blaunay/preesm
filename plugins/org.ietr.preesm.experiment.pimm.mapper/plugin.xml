<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.ietr.dftools.workflow.tasks">
      <task
            id="org.ietr.preesm.experiment.pimm.mapper.MultiSDFListSchedulingMapping"
            name="Multi Kwok list scheduling algorithm"
            type="org.ietr.preesm.experiment.pimm.mapper.MultiSDFListSchedulingMapping">
         <inputs>
            <input
                  id="SDFs"
                  object="java.util.Set&lt;org.ietr.dftools.algorithm.model.sdf.SDFGraph&gt;">
            </input>
            <input
                  id="scenario"
                  object="org.ietr.preesm.core.scenario.PreesmScenario">
            </input>
            <input
                  id="architecture"
                  object="org.ietr.dftools.architecture.slam.Design">
            </input>
         </inputs>
         <outputs>
            <output
                  id="DAGs"
                  object="java.util.Set&lt;org.ietr.dftools.algorithm.model.dag.DirectedAcyclicGraph&gt;">
            </output>
            <output
                  id="ABCs"
                  object="java.util.Set&lt;org.ietr.preesm.mapper.abc.IAbc&gt;">
            </output>
         </outputs>
      </task>
      <task
            id="org.ietr.preesm.experiment.pimm.mapper.MultiStatEditorTransform"
            name="Multi Gantt plot transform"
            type="org.ietr.preesm.experiment.pimm.mapper.MultiStatEditorTransform">
         <inputs>
            <input
                  id="scenario"
                  object="org.ietr.preesm.core.scenario.PreesmScenario">
            </input>
            <input
                  id="ABCs"
                  object="java.util.Set&lt;org.ietr.preesm.mapper.abc.IAbc&gt;">
            </input>
         </inputs>
      </task>
   </extension>

</plugin>
