<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.ietr.preesm.core.tasks">
      <task
            id="org.ietr.preesm.plugin.mapper.fast"
            name="Kwok FAST algorithm"
            type="org.ietr.preesm.plugin.mapper.FASTTransformation">
      </task>
      <task
            id="org.ietr.preesm.plugin.mapper.pfast"
            name="Kwok PFAST algorithm (parallel version of FAST)"
            type="org.ietr.preesm.plugin.mapper.PFASTTransformation">
      </task>
      <task
            id="org.ietr.preesm.plugin.mapper.listscheduling"
            name="Kwok list scheduling algorithm"
            type="org.ietr.preesm.plugin.mapper.ListSchedulingTransformation">
      </task>
      <task
            id="org.ietr.preesm.plugin.mapper.pgenetic"
            name="Parallel genetic scheduling algorithm"
            type="org.ietr.preesm.plugin.mapper.PGeneticTransformation">
      </task>
      <task
            id="org.ietr.preesm.plugin.mapper.standardgenetic"
            name="Standard genetic scheduling algorithm"
            type="org.ietr.preesm.plugin.mapper.StandardGeneticTransformation">
      </task>
      <task
            id="org.ietr.preesm.plugin.mapper.communicationcontentiouslistscheduling"
            name="communication contentious list scheduler"
            type="org.ietr.preesm.plugin.mapper.CommunicationContentiousListSchedulingTransformation">
      </task>
      <task
            id="org.ietr.preesm.plugin.mapper.plot.GanttExporter"
            name="Gantt Exporter"
            type="org.ietr.preesm.plugin.mapper.plot.GanttExporter">
      </task>
   </extension>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            describer="org.ietr.preesm.plugin.mapper.plot.GanttContentDescriber"
            file-extensions="gantt"
            id="org.ietr.preesm.mapper.gantttype"
            name="gantt"
            priority="normal">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.ietr.preesm.plugin.mapper.plot.ImplementationEditor"
            default="false"
            id="org.ietr.preesm.plugin.mapper.plot.ImplementationEditor"
            name="Implementation editor">
         <contentTypeBinding
               contentTypeId="org.eclipse.core.runtime.xml">
         </contentTypeBinding>
      </editor>
   </extension>

</plugin>
