        </node>
        <edge source="Read_YUV" sourceport="U"
            target="substract_pic_slices" targetport="U1">
            <data key="edge_prod">xsize/2*ysize/2</data>
            <data key="edge_delay">0</data>
            <data key="edge_cons">xsize/2*ysize/2</data>
            <data key="data_type">uchar</data>
        </edge>
        <edge source="Read_YUV" sourceport="V"
